CREATE TABLE player (
 
player_id NUMBER NOT NULL UNIQUE  ,
player_name varchar2(50) UNIQUE ,
player_age NUMBER

);

CREATE TABLE Manger (
 
Manger_id NUMBER NOT NULL   ,
Manger_name varchar2(50)  ,
Manger_salary varchar2(50),
CONSTRAINT  composite_unique UNIQUE (Manger_id ,Manger_name )

);



CREATE TABLE Manger (
 
Manger_id NUMBER CONSTRAINT manger_id_pk PRIMARY KEY  ,
Manger_name varchar2(50)  ,
Manger_age NUMBER

);

